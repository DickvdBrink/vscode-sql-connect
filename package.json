{
	"name": "vscode-sql-connect",
	"displayName": "vscode-sql-connect",
	"description": "Connect to SQL Server from vscode",
	"version": "0.0.1",
	"publisher": "dvdbrink",
	"engines": {
		"vscode": "^0.10.1"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:extension.sqlConnect",
		"onCommand:extension.sqlDisconnect"
	],
	"main": "./out/src/extension",
	"contributes": {
		"commands": [{
			"command": "extension.sqlConnect",
			"title": "Connect SQL"
		},
		{
			"command": "extension.sqlDisconnect",
			"title": "Disconnect SQL"
		}]
	},
	"scripts": {
		"vscode:prepublish": "node ./node_modules/vscode/bin/compile",
		"compile": "node ./node_modules/vscode/bin/compile -watch -p ./"
	},
	"dependencies": {
		"mssql": "2.3.2"
	},
	"devDependencies": {
		"typescript": "^1.6.2",
		"vscode": "0.10.x"
	}
}